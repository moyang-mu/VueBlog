A,aAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,UAAA,iBAAA,EAAA,IADA;AAEA,UAAA,gBAAA,EAAA,IAFA;AAGA,UAAA,IAAA,EAAA;AAHA,SAAA,EAIA,IAJA,CAIA,YAAA;AAGA,UAAA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,kBAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AAEA,gBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA;AACA,gBAAA,IAAA,EAAA,SADA;AAEA,gBAAA,OAAA,EAAA;AAFA,eAAA;AAIA,aALA,MAKA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA;AACA,gBAAA,IAAA,EAAA,MADA;AAEA,gBAAA,OAAA,EAAA;AAFA,eAAA;AAIA;;AACA,YAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAAA,WAAA;AAEA,WAhBA;AAmBA,SA1BA,EA0BA,KA1BA,CA0BA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,MADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA,SA/BA;AAgCA,OAjCA,MAiCA;AACA,aAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA;AAEA;AAzDA,GAXA;AAsEA,EAAA,OAtEA,qBAsEA;AACA,SAAA,IAAA,CAAA,CAAA;AACA;AAxEA,CAAA","sourcesContent":["<template>\r\n  <div class=\"mcontaner\">\r\n    <Header></Header>\r\n\r\n    <div class=\"block\">\r\n      <el-timeline>\r\n\r\n        <el-timeline-item :timestamp=\"blog.created\" placement=\"top\" v-for=\"blog in blogs\">\r\n          <el-card>\r\n            <h4>\r\n              <router-link :to=\"{name: 'BlogDetail', params: {blogId: blog.id}}\">\r\n                {{blog.title}}\r\n              </router-link>\r\n            </h4>\r\n\r\n              <p>{{blog.description}}</p>\r\n                <router-link :to=\"{name: 'BlogEdit', params: {blogId: blog.id}}\" >\r\n                  <el-button type=\"primary\" icon=\"el-icon-edit\" circle size=\"small\" class=\"anniu\" >\r\n                  </el-button>\r\n\r\n\r\n                </router-link>\r\n              <el-button type=\"danger\" icon=\"el-icon-delete\" circle size=\"small\" class=\"anniu\" @click=\"shan(blog.id)\"></el-button>\r\n\r\n\r\n          </el-card>\r\n        </el-timeline-item>\r\n\r\n      </el-timeline>\r\n\r\n      <el-pagination class=\"mpage\"\r\n                     background\r\n                     layout=\"prev, pager, next\"\r\n                     :current-page=\"currentPage\"\r\n                     :page-size=\"pageSize\"\r\n                     :total=\"total\"\r\n                     @current-change=page>\r\n      </el-pagination>\r\n\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import Header from \"../components/Header\";\r\n\r\n  export default {\r\n    name: \"Blogs.vue\",\r\n    components: {Header},\r\n    data() {\r\n      return {\r\n        blogs: {},\r\n        currentPage: 1,\r\n        total: 0,\r\n        pageSize: 5\r\n      }\r\n    },\r\n    methods: {\r\n      page(currentPage) {\r\n        const _this = this\r\n        _this.$axios.get(\"/blogs?currentPage=\" + currentPage).then(res => {\r\n          console.log(res)\r\n          _this.blogs = res.data.data.records\r\n          _this.currentPage = res.data.data.current\r\n          _this.total = res.data.data.total\r\n          _this.pageSize = res.data.data.size\r\n\r\n        })\r\n      },\r\n        shan(blogId){\r\n          console.log(blogId)\r\n          const _this = this\r\n\r\n          const token = localStorage.getItem(\"token\")\r\n          if (token){\r\n            this.$confirm('此操作将永久删除该博客, 是否继续?', '提示', {\r\n              confirmButtonText: '确定',\r\n              cancelButtonText: '取消',\r\n              type: 'warning'\r\n            }).then(() => {\r\n\r\n\r\n              this.$axios.get('/blog/delete/' + blogId).then(res => {\r\n                console.log(res)\r\n\r\n                if (res.data.code == 200 ){\r\n                  this.$message({\r\n                    type: 'success',\r\n                    message: '删除成功!'\r\n                  });\r\n                }else{\r\n                  this.$message({\r\n                    type: 'info',\r\n                    message: '删除失败'\r\n                  });\r\n                }\r\n                this.page(this.currentPage);\r\n\r\n              })\r\n\r\n\r\n            }).catch(() => {\r\n              this.$message({\r\n                type: 'info',\r\n                message: '已取消删除'\r\n              });\r\n            });\r\n          }else {\r\n            this.$message({\r\n              type: 'warning',\r\n              message: '你没有权限删除，请先登陆'\r\n            });\r\n          }\r\n\r\n        }\r\n    },\r\n    created() {\r\n      this.page(1)\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n  .mpage {\r\n    margin: 0 auto;\r\n    text-align: center;\r\n  }\r\n  .anniu{\r\n        float: right;\r\n        /*float: right;*/\r\n      margin-right: 10px;\r\n      margin-bottom: 10px;\r\n  }\r\n\r\n</style>"],"sourceRoot":"src/views"}]}